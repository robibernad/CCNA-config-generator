# Backend Configuration

# Disable Mock Mode to connect to real GNS3 server
MOCK_MODE=False

# CORS Origins (comma-separated)
CORS_ORIGINS=http://localhost:3000,http://127.0.0.1:3000

# Session TTL (in seconds)
SESSION_TTL=7200

# Artifacts
ARTIFACTS_DIR=./artifacts
ENABLE_ARTIFACTS=False

# Logging
LOG_LEVEL=INFO

# API Prefix
API_PREFIX=/api

# Security
ALLOW_LOCALHOST_GNS3=True

# AI Validation (Optional)
OPENAI_API_KEY=
OPENAI_MODEL=gpt-4
